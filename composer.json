{
    "name": "dealroadshow/k8s-bundle",
    "description": "Integrates Dealroadshow K8S framework with Symfony 5",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Petr Buchin",
            "email": "petrbuchyn@gmail.com"
        },
        {
            "name": "Bogdan Pasichnyk",
            "email": "pasechnikbs@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/petr-buchin/ProxyManager"
        }
    ],
    "require": {
        "php": ">=8.0",
        "symfony/console": "5.3.*",
        "symfony/dependency-injection": "5.3.*",
        "symfony/config": "5.3.*",
        "symfony/http-kernel": "5.3.*",
        "dealroadshow/k8s-framework": "dev-master",
        "symfony/event-dispatcher": "^5.3"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Dealroadshow\\Bundle\\K8SBundle\\": "src/"
        }
    }
}
